                <ext:label>Gruppe</ext:label>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="member" type="com.ibm.ws.security.registry.basic.config.group.member-factory">
